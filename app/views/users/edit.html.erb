<div id="edit_user_form">
  <%= form_for(@user) do |f| %>
   <%= f.label :username %>
   <%= f.text_field :username %>
   <%= f.label :first_name %>
   <%= f.text_field :first_name %>
   <%= f.label :last_name %>
   <%= f.text_field :last_name %>
   <%= f.label :email %>
   <%= f.text_field :email %>
   <%= f.label :pic_url%>
   <%= f.text_field :pic_url %>
   <%= f.label :url %>
   <%= f.text_field :url %>
   <%= f.label :country %>
   <%= f.text_field :country %>
   <%= f.label :bio %>
   <%= f.text_area :bio %>
   <%= f.submit "Update" %>
  <% end %>
</div>
