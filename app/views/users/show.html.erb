<%= @user.username %><br>
<%= @user.country %><br>
<%= image_tag @user.pic_url %><br>
<%= link_to "Website", @user.url %><br>

<br>
User's Snippets<br>
<% @user.snippets.each do |snippet| %>
  <%= link_to snippet.content, snippet_path(snippet) %>
<% end %>
<br><br>
User's stories<br>
<% @user.all_published.each do |published| %>
  <%= link_to published.title, story_path(published) %>
<% end %>